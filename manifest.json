{
  "manifest_version": 3,
  "name": "Zonaprop Price per mÂ²",
  "version": "1.0",
  "description": "Displays price per square meter for Zonaprop properties",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.zonaprop.com.ar/*",
        "*://zonaprop.com.ar/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "host_permissions": [
    "*://*.zonaprop.com.ar/*",
    "*://zonaprop.com.ar/*",
    "https://api.bluelytics.com.ar/*"
  ],
  "permissions": [
    "activeTab"
  ]
}