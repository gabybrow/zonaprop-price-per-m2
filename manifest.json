
{
  "manifest_version": 3,
  "name": "Zonaprop Price per mÂ²",
  "version": "1.0",
  "description": "Displays price per square meter for Zonaprop properties",
  "icons": {
    "48": "icon.svg",
    "128": "icon.svg"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.zonaprop.com.ar/*",
        "*://zonaprop.com.ar/*"
      ],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "host_permissions": [
    "*://*.zonaprop.com.ar/*",
    "*://zonaprop.com.ar/*",
    "https://api.bluelytics.com.ar/*"
  ],
  "permissions": [
    "activeTab"
  ]
}
